{
  "name": "projectbackend",
  "version": "1.0.0",
  "description": "test project",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nodemon src/main.ts",
    "test": "jest --watch",
    "build": "tsc -p tsconfig.json",
    "migration-dev": "ts-node ./src/prisma/generator.ts && npx prisma migrate dev",
    "migration": "ts-node ./src/prisma/generator.ts && npx prisma migrate deploy"
  },
  "author": "williams",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "@types/crypto-js": "^4.0.2",
    "@types/mailgun-js": "^0.22.12",
    "@types/node": "^15.14.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fastify": "^3.21.0",
    "fastify-auth": "^1.1.0",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-grant": "^5.4.8",
    "fastify-helmet": "^5.3.2",
    "fastify-postgres": "^3.6.0",
    "fastify-rate-limit": "^5.6.2",
    "fastify-session": "^5.2.1",
    "fastify-swagger": "^4.11.0",
    "jest-mock-extended": "^2.0.4",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "paypal-rest-sdk": "^1.8.1",
    "pg": "^8.7.1",
    "qrcode": "^1.4.4",
    "speakeasy": "^2.0.0",
    "supertest": "^6.1.6",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.4",
    "nodemon": "^2.0.12",
    "prisma": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "\\.pnp\\.[^\\/]+$"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  }
}
